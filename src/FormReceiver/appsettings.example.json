{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInfo": {
    "AppName": "YourAppName",
    "WhatsApp": "YourWhatsApp",
    "Site": "https://yourdomain.com"
  },
  "SmtpInfo": {
    "From": "your-email@example.com",
    "To": "recipient@example.com",
    "SmtpServer": "smtp.example.com",
    "SmtpPort": 587,
    "SmtpUser": "your-smtp-user@example.com",
    "SmtpPass": "your-smtp-password"
  },
  "WhatsAppEndpoints": {
    "ZapHub": {
      "Token": "your-whatsapp-token",
      "Url": "https://graph.facebook.com/vXX.X/your-whatsapp-id/messages"
    }
  }
}
